{
	"env": {
		// "browser": true,
		"es6": true,
		"node": true
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": ["tsconfig.json"]
	},
	"extends": ["airbnb-typescript/base", "plugin:prettier/recommended", "node"],
	"plugins": ["import", "prettier"],
	"rules": {
		"prettier/prettier": "off",
		"no-console": "warn",
		"no-unused-vars": "warn",
		"no-dupe-class-members": "off",
		"indent": ["error", "tab"],
		"linebreak-style": ["warn", "unix"],
		"quotes": ["error", "double"],
		"semi": ["error", "never"]
	}
}
